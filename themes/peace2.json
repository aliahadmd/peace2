{
    "name": "peace2",
    "type": "dark",
    "colors": {
        "editor.background": "#000000",
        "editor.foreground": "#839496",
        "editor.lineHighlightBackground": "#002b36",
        "editor.selectionBackground": "#003366",
        "editor.selectionForeground": "#FFFFFF",
        "activityBar.background": "#000000",
        "activityBar.foreground": "#839496",
        "activityBarBadge.background": "#002b36",
        "sideBar.background": "#000000",
        "sideBar.foreground": "#839496",
        "sideBarTitle.foreground": "#BBBBBB",
        "statusBar.background": "#000000",
        "statusBar.foreground": "#FFFFFF",
        "statusBar.noFolderBackground": "#002b36",
        "titleBar.activeBackground": "#000000",
        "titleBar.activeForeground": "#839496",
        "tab.activeBackground": "#000000",
        "tab.inactiveBackground": "#1a1a1a",
        "tab.activeForeground": "#FFFFFF",
        "tab.inactiveForeground": "#888888",
        "editorGroupHeader.tabsBackground": "#000000",
        "tab.border": "#333333"
    },
    "tokenColors": [
    {
        "name": "Comments",
        "scope": ["comment", "punctuation.definition.comment"],
        "settings": {
            "foreground": "#0f5f77"
        }
    },
    {
        "name": "Strings",
        "scope": ["string", "string.quoted"],
        "settings": {
            "foreground": "#A2734C"
        }
    },
    {
        "name": "Numbers",
        "scope": ["constant.numeric", "constant.integer", "constant.float"],
        "settings": {
            "foreground": "#bf9a64"
        }
    },
    {
        "name": "Booleans and Constants",
        "scope": ["constant.language", "constant.character"],
        "settings": {
            "foreground": "#d7dfe0",
            "fontStyle": "bold"
        }
    },
    {
        "name": "Keywords",
        "scope": ["keyword", "storage.type", "storage.modifier"],
        "settings": {
            "foreground": "#d7dfe0",
            "fontStyle": "bold"
        }
    },
    {
        "name": "Functions",
        "scope": ["entity.name.function", "support.function"],
        "settings": {
            "foreground": "#F66151"
        }
    },
    {
        "name": "Variables",
        "scope": ["variable", "support.variable"],
        "settings": {
            "foreground": "#88b0b8",
            "fontStyle": "italic"
        }
    },
    {
        "name": "Constants and Enums",
        "scope": ["variable.other.constant", "variable.other.enummember"],
        "settings": {
            "foreground": "#bf9a64",
            "fontStyle": "bold"
        }
    },
    {
        "name": "Classes and Types",
        "scope": ["entity.name.type", "entity.name.class", "entity.name.namespace"],
        "settings": {
            "foreground": "#A347BA"
        }
    },
    {
        "name": "Interfaces",
        "scope": ["entity.name.type.interface"],
        "settings": {
            "foreground": "#A347BA",
            "fontStyle": "italic"
        }
    },
    {
        "name": "Attributes and Tags",
        "scope": ["entity.other.attribute-name", "entity.name.tag"],
        "settings": {
            "foreground": "#88b0b8",
            "fontStyle": "bold"
        }
    },
    {
        "name": "Operators",
        "scope": ["keyword.operator", "keyword.control"],
        "settings": {
            "foreground": "#d7dfe0"
        }
    },
    {
        "name": "Punctuation",
        "scope": [
            "punctuation.definition.string",
            "punctuation.definition.variable",
            "punctuation.separator",
            "punctuation.terminator"
        ],
        "settings": {
            "foreground": "#6c8a96"
        }
    },
    {
        "name": "JSON Property Names (Keys)",
        "scope": "string.quoted.double.json",
        "settings": {
            "foreground": "#88b0b8"
        }
    },
    {
        "name": "JSON Values (Strings)",
        "scope": "string.quoted.double.json string",
        "settings": {
            "foreground": "#A2734C"
        }
    },
    {
        "name": "JSON Values (Numbers)",
        "scope": "constant.numeric.json",
        "settings": {
            "foreground": "#bf9a64"
        }
    },
    {
        "name": "JSON Values (Booleans)",
        "scope": "constant.language.json",
        "settings": {
            "foreground": "#d7dfe0",
            "fontStyle": "bold"
        }
    },
    {
        "name": "JSON Punctuation",
        "scope": [
            "punctuation.definition.string.begin.json",
            "punctuation.definition.string.end.json",
            "punctuation.separator.dictionary.key-value.json"
        ],
        "settings": {
            "foreground": "#6c8a96"
        }
    },
    {
        "name": "HTML Tags",
        "scope": ["entity.name.tag.html"],
        "settings": {
            "foreground": "#A347BA"
        }
    },
    {
        "name": "HTML Attributes",
        "scope": ["entity.other.attribute-name.html"],
        "settings": {
            "foreground": "#88b0b8",
            "fontStyle": "italic"
        }
    },
    {
        "name": "CSS Selectors",
        "scope": ["entity.name.tag.css"],
        "settings": {
            "foreground": "#A347BA"
        }
    },
    {
        "name": "CSS Properties",
        "scope": ["support.type.property-name.css"],
        "settings": {
            "foreground": "#F66151"
        }
    },
    {
        "name": "CSS Values",
        "scope": ["constant.numeric.css", "constant.other.color.rgb-value.css"],
        "settings": {
            "foreground": "#bf9a64"
        }
    }
]

}
